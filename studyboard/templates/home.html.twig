        </div>
        <div id="close" onclick="togglenav()"></div>
        <!--End -->

        <!-- Dashboard -->
        <div class="quicklinks" id="qlinks">
            <div>
                <a href="./">
                    <i class="material-icons">home</i>
                </a>
                <span class="tooltip">Home</span>
            </div>
            <div>
                <a href="foren">
                    <i class="material-icons">announcement</i>
                </a>
                <span class="tooltip">Foren</span>
            </div>
            <div>
                <a href="#account">
                    <i class="material-icons">face</i>
                </a>
                <span class="tooltip">Freunde</span>
            </div>
            <div>
                <a href="#settings">
                    <i class="material-icons">settings</i>
                </a>
                <span class="tooltip">Settings</span>
            </div>
            <div id="logout">
                <a href="#logout" onclick="togglelogoutalert()">
                    <i class="material-icons">power_settings_new</i>
                </a>
                <span class="tooltip">Logout</span>
            </div>
        </div>

        <script src="./script/global/navbar.js"></script>
        <script src="./script/index/dashboard.js"></script>
        <script src="./script/global/logout/logout.js"></script>
        <script src="./script/global/logout/refer.js"></script>
